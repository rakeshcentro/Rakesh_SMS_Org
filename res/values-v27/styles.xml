    </style>
    <style name="Base.V27.Theme.GoogleMaterial3.Light.BottomSheetDialog" parent="@style/Base.V21.Theme.GoogleMaterial3.Light.BottomSheetDialog">
        <item name="android:navigationBarColor">@color/gm3_ref_palette_white</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@android:color/transparent</item>
    </style>
    <style name="BugleDarkNavBarTheme" parent="@style/BugleToolbarTheme">
        <item name="android:navigationBarColor">@color/navigation_bar_color</item>
        <item name="android:navigationBarDividerColor">@color/navigation_bar_divider_color</item>
    </style>
    <style name="OneGoogle.Dialog.Base.V27" parent="@style/OneGoogle.Dialog.Base.V23">
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="OneGoogle.Dialog.Material.Base.V27" parent="@style/OneGoogle.Dialog.Material.Base.V23">
        <item name="android:windowLightNavigationBar">false</item>
    </style>
